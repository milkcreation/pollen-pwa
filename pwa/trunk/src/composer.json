  "keywords": [
    "presstify",
    "plugins",
    "wordpress",
    "pwa"
  ],
  "time": "2021-01-07",
  "license": "proprietary",
  "authors": [
    {
      "name": "Jordy Manner",
      "email": "jordy@presstify.com",
